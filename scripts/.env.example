# Environment variables for local testing
# Copy this file to .env and update the values

# Backend URL (use your Vercel URL for production testing)
API_BASE_URL=https://wouri-ashen.vercel.app

# Admin API Key (get from Vercel environment variables)
ADMIN_API_KEY=your_admin_api_key_here

# Alert threshold in milliseconds (optional, default: 5000)
ALERT_THRESHOLD_MS=5000

# Enable success alerts (optional, default: false)
ALERT_ON_SUCCESS=false

# Slack webhook URL (optional)
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Discord webhook URL (optional)
# DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/WEBHOOK/URL
